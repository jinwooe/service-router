package com.skcc.servicerouter.service;

public class RouteInfo {
    private long cameraId;
    private String scheme; // tcp, http
    private String ip;
    private int port;
    private String path;
    private String token;

    public RouteInfo() {

    }

    public RouteInfo(long cameraId, String scheme, String ip, int port, String path, String token) {
        this.cameraId = cameraId;
        this.scheme = scheme;
        this.ip = ip;
        this.port = port;
        this.path = path;
        this.token = token;
    }

    public long getCameraId() {
        return cameraId;
    }

    public String getScheme() {
        return scheme;
    }

    public String getIp() {
        return ip;
    }

    public int getPort() {
        return port;
    }

    public String getPath() {
        return path;
    }

    public String getToken() {
        return token;
    }
}
